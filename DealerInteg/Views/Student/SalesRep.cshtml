@model IEnumerable<DealerPortalCRM.ViewModels.SalesDashboardViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SalesRepName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalNew)
        </th>
     
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SalesRepName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalNew)
            </td>

           
        </tr>
    }

</table>
