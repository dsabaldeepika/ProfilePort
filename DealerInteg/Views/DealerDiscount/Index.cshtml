@model IEnumerable<DealerPortalCRM.ViewModels.DealerDiscountViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Dealer Discount</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Discount
        </th>
        <th>
            Dealer Type
        </th>
        <th>
            State Name
        </th>
        <th>
            Modified By
        </th>
        <th>
            Modified Date
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelitem => item.DealerDiscountDiscount) %
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DealerTypeType)
        </td>
        <td>
            @Html.DisplayFor(modelitem => item.StateCodeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DealerDiscountModifiedByID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DealerDiscountModifiedDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.DealerDiscountID }) 
        </td>
    </tr>
}

</table>
